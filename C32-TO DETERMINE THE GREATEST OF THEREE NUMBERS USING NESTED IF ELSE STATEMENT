//INPUT
 #include<stdio.h>
int main(){
    int a,b,c;
    printf("Enter three numbers: "); 
    scanf("%d", &a);
    scanf("%d", &b);
    scanf("%d", &c);
    if(a>b){
        if(a>c){
        printf("a is greatest: %d", a);
        }
        else{
        printf("c is greatest: %d", c);
        }
    }
    else{
        if(b>c){
        printf("b is greatest: %d", b);
        }
        else{
        printf("c is greatest: %d", c);
        }
    }
    return 0;
}
//OUTPUT
Enter three numbers: 3
4
5
c is greatest: 5
